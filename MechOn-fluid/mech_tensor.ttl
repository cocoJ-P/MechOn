@prefix : <http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/> .

<http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9> rdf:type owl:Ontology ;
                                                                            owl:imports <http://www.w3.org/2004/02/skos/core> .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/defineOverSpatialDomain
:defineOverSpatialDomain rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/describesTensor
:describesTensor rdf:type owl:ObjectProperty ;
                 owl:inverseOf :isDescribedBy ;
                 rdfs:domain :TensorInvariant ;
                 rdfs:range :TensorField ;
                 rdfs:label "describs tensor"@en ;
                 <http://www.w3.org/2004/02/skos/core#altLabel> "描述张量"@zh .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/hasCoordinateSystem
:hasCoordinateSystem rdf:type owl:ObjectProperty ;
                     rdfs:domain :TensorField ;
                     rdfs:range :CoordinateSystem ;
                     rdfs:label "has coordinate system"@en ;
                     <http://www.w3.org/2004/02/skos/core#altLabel> "使用坐标系"@zh .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/hasSymmetry
:hasSymmetry rdf:type owl:ObjectProperty ;
             rdfs:domain :TensorField ;
             rdfs:range :SymmetryType ;
             rdfs:label "has symmetry"@en ;
             <http://www.w3.org/2004/02/skos/core#altLabel> "具有对称性"@zh .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/hasTensorOrder
:hasTensorOrder rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :TensorField .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/isDescribedBy
:isDescribedBy rdf:type owl:ObjectProperty ;
               rdfs:domain :TensorField ;
               rdfs:range :TensorInvariant ;
               rdfs:label "is described by invariant"@en .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/refersToQUDTQuantityKind
:refersToQUDTQuantityKind rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf owl:topObjectProperty ;
                          rdfs:domain :ZeroOrderTensorField .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/hasTensorOrder
:hasTensorOrder rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:range xsd:nonNegativeInteger .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/AntiSymmetricTensor
:AntiSymmetricTensor rdf:type owl:Class ;
                     rdfs:subClassOf :SecondOrderTensorField ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasSymmetry ;
                                       owl:hasValue :AntiSymmetric
                                     ] ;
                     rdfs:label "Anti-symmetric tensor"@en ;
                     <http://www.w3.org/2004/02/skos/core#altLabel> "反对称张量"@zh .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/Compressibility
:Compressibility rdf:type owl:Class ;
                 rdfs:subClassOf :ZeroOrderTensorField ;
                 rdfs:label "Compressibility"@en ;
                 <http://www.w3.org/2004/02/skos/core#altLabel> "压缩率"@zh .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/CoordinateSystem
:CoordinateSystem rdf:type owl:Class ;
                  rdfs:comment "Reference frame or coordinate chart used to express tensor components."@en ;
                  rdfs:label "Coordinate System"@en ;
                  <http://www.w3.org/2004/02/skos/core#altLabel> "坐标系"@zh .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/Density
:Density rdf:type owl:Class ;
         rdfs:subClassOf :ZeroOrderTensorField ;
         rdfs:label "Density"@en ;
         <http://www.w3.org/2004/02/skos/core#altLabel> "密度"@zh .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/DynamicViscosity
:DynamicViscosity rdf:type owl:Class ;
                  rdfs:subClassOf :Viscosity ;
                  rdfs:label "Dynamic Viscosity"@en ;
                  <http://www.w3.org/2004/02/skos/core#altLabel> "动力粘度"@zh .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/Entropy
:Entropy rdf:type owl:Class ;
         rdfs:subClassOf :ZeroOrderTensorField ;
         rdfs:label "Entropy"@en ;
         <http://www.w3.org/2004/02/skos/core#altLabel> "熵"@zh .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/FirstInvariant
:FirstInvariant rdf:type owl:Class ;
                rdfs:subClassOf :TensorInvariant .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/FirstOrderTensorField
:FirstOrderTensorField rdf:type owl:Class ;
                       rdfs:subClassOf :TensorField ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasCoordinateSystem ;
                                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass :CoordinateSystem
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasTensorOrder ;
                                         owl:hasValue 1
                                       ] ;
                       owl:disjointWith :SecondOrderTensorField ,
                                        :ZeroOrderTensorField ;
                       rdfs:comment "Tensor fields of order 1 (vector quantities)."@en ;
                       rdfs:label "Vector Tensor"@en ;
                       <http://www.w3.org/2004/02/skos/core#altLabel> "一阶张量"@zh .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/InternalEnergy
:InternalEnergy rdf:type owl:Class ;
                rdfs:subClassOf :ZeroOrderTensorField ;
                rdfs:label "Internal Energy"@en ;
                <http://www.w3.org/2004/02/skos/core#altLabel> "内能"@zh .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/KinematicViscosity
:KinematicViscosity rdf:type owl:Class ;
                    rdfs:subClassOf :Viscosity ;
                    rdfs:label "Kinematic Viscosity"@en ;
                    <http://www.w3.org/2004/02/skos/core#altLabel> "运动粘度"@zh .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/KineticEnergy
:KineticEnergy rdf:type owl:Class ;
               rdfs:subClassOf :ZeroOrderTensorField ;
               rdfs:label "Kinetic Energy"@en ;
               <http://www.w3.org/2004/02/skos/core#altLabel> "动能"@zh .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/MagneticFluxDensity
:MagneticFluxDensity rdf:type owl:Class ;
                     rdfs:subClassOf :FirstOrderTensorField ;
                     rdfs:label "Magnetic flux density"@en ;
                     <http://www.w3.org/2004/02/skos/core#altLabel> "磁感应强度"@zh .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/Pressure
:Pressure rdf:type owl:Class ;
          rdfs:subClassOf :ZeroOrderTensorField ;
          rdfs:label "pressure"@en ;
          <http://www.w3.org/2004/02/skos/core#altLabel> "压强"@zh .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/SecondInvariant
:SecondInvariant rdf:type owl:Class ;
                 rdfs:subClassOf :TensorInvariant .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/SecondOrderTensorField
:SecondOrderTensorField rdf:type owl:Class ;
                        rdfs:subClassOf :TensorField ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :hasTensorOrder ;
                                          owl:hasValue 2
                                        ] ;
                        owl:disjointWith :ZeroOrderTensorField ;
                        rdfs:comment "Tensor fields of order 2 (matrices)."@en ;
                        rdfs:label "Matrix Tensor"@en ;
                        <http://www.w3.org/2004/02/skos/core#altLabel> "二阶张量"@zh .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/StrainRateTensor
:StrainRateTensor rdf:type owl:Class ;
                  rdfs:subClassOf :SymmetricTensor .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/StressTensor
:StressTensor rdf:type owl:Class ;
              rdfs:subClassOf :SymmetricTensor .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/SymmetricTensor
:SymmetricTensor rdf:type owl:Class ;
                 rdfs:subClassOf :SecondOrderTensorField ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasSymmetry ;
                                   owl:hasValue :Symmetric
                                 ] ;
                 rdfs:label "Symmetric tensor"@en ;
                 <http://www.w3.org/2004/02/skos/core#altLabel> "对称张量"@zh .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/SymmetryType
:SymmetryType rdf:type owl:Class ;
              rdfs:label "Symmetry type"@en ;
              <http://www.w3.org/2004/02/skos/core#altLabel> "对称性类型"@zh .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/Temperature
:Temperature rdf:type owl:Class ;
             rdfs:subClassOf :ZeroOrderTensorField ;
             rdfs:label "Temperature"@en ;
             <http://www.w3.org/2004/02/skos/core#altLabel> "温度"@zh .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/TensorField
:TensorField rdf:type owl:Class .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/TensorInvariant
:TensorInvariant rdf:type owl:Class .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/ThermalConductivity
:ThermalConductivity rdf:type owl:Class ;
                     rdfs:subClassOf :ZeroOrderTensorField ;
                     rdfs:label "Thermal Conductivity"@en ;
                     <http://www.w3.org/2004/02/skos/core#altLabel> "热导率"@zh .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/ThermalExpansion
:ThermalExpansion rdf:type owl:Class ;
                  rdfs:subClassOf :ZeroOrderTensorField ;
                  rdfs:label "Thermal Expansion"@en ;
                  <http://www.w3.org/2004/02/skos/core#altLabel> "热膨胀系数"@zh .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/ThirdInvariant
:ThirdInvariant rdf:type owl:Class ;
                rdfs:subClassOf :TensorInvariant .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/Velocity
:Velocity rdf:type owl:Class ;
          rdfs:subClassOf :FirstOrderTensorField ;
          rdfs:label "Velocity"@en ;
          <http://www.w3.org/2004/02/skos/core#altLabel> "速度"@zh .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/Viscosity
:Viscosity rdf:type owl:Class ;
           rdfs:subClassOf :ZeroOrderTensorField ;
           rdfs:label "Viscosity"@en ;
           <http://www.w3.org/2004/02/skos/core#altLabel> "粘度"@zh .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/VorticityTensor
:VorticityTensor rdf:type owl:Class ;
                 rdfs:subClassOf :AntiSymmetricTensor .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/ZeroOrderTensorField
:ZeroOrderTensorField rdf:type owl:Class ;
                      rdfs:subClassOf :TensorField ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasTensorOrder ;
                                        owl:hasValue 0
                                      ] ;
                      rdfs:comment "A tensor field of order 0 (scalar physical quantity)."@en ;
                      rdfs:label "Scalar Tensor"@en ;
                      <http://www.w3.org/2004/02/skos/core#altLabel> "零阶张量"@zh ;
                      <http://www.w3.org/2004/02/skos/core#definition> "Scalar tensor fields such as pressure, temperature, density, etc."@en ;
                      <http://www.w3.org/2004/02/skos/core#example> :Pressure ,
                                                                    :Temperature .


#################################################################
#    Individuals
#################################################################

###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/AntiSymmetric
:AntiSymmetric rdf:type owl:NamedIndividual ,
                        :SymmetryType ;
               rdfs:label "anti-symmetric"@en .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/CartesianCS
:CartesianCS rdf:type owl:NamedIndividual ,
                      :CoordinateSystem ;
             rdfs:label "Cartesian coordinate system"@en .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/CylindricalCS
:CylindricalCS rdf:type owl:NamedIndividual ,
                        :CoordinateSystem ;
               rdfs:label "Cylindrical coordinate system"@en .


###  http://www.semanticweb.org/jiashun/ontologies/2025/6/untitled-ontology-9/Symmetric
:Symmetric rdf:type owl:NamedIndividual ,
                    :SymmetryType ;
           rdfs:label "symmetric"@en .


#################################################################
#    Annotations
#################################################################

:hasTensorOrder rdfs:label "tensor order"@en ;
                <http://www.w3.org/2004/02/skos/core#altLabel> "张量阶数"@zh .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :FirstOrderTensorField
                :SecondOrderTensorField
                :ZeroOrderTensorField
              )
] .


