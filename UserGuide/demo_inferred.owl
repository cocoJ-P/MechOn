<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://www.semanticweb.org/jiashun/ontologies/2025/6/fluidtest/"
         xmlns="http://www.semanticweb.org/jiashun/ontologies/2025/6/fluidtest/">

<owl:Ontology rdf:about="http://www.semanticweb.org/jiashun/ontologies/2025/6/fluidtest"/>

<owl:ObjectProperty rdf:about="hasVelocityField">
  <rdfs:domain rdf:resource="Fluid"/>
  <rdfs:range rdf:resource="VelocityField"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">流体拥有速度场</rdfs:comment>
</owl:ObjectProperty>

<owl:Class rdf:about="DivergenceFreeVelocityField"/>

<owl:Class rdf:about="Fluid">
  <owl:equivalentClass rdf:resource="IncompressibleFluid"/>
  <owl:equivalentClass>
    <owl:Restriction>
      <owl:onProperty rdf:resource="hasVelocityField"/>
      <owl:someValuesFrom rdf:resource="DivergenceFreeVelocityField"/>
    </owl:Restriction>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="VelocityField"/>

<owl:Class rdf:about="IncompressibleFluid">
  <rdfs:subClassOf rdf:resource="Fluid"/>
  <owl:equivalentClass rdf:resource="Fluid"/>
</owl:Class>

<owl:NamedIndividual rdf:about="fluid1">
  <rdf:type rdf:resource="Fluid"/>
  <hasVelocityField rdf:resource="vfield1"/>
  <rdf:type rdf:resource="IncompressibleFluid"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="vfield1">
  <rdf:type rdf:resource="DivergenceFreeVelocityField"/>
  <rdf:type rdf:resource="VelocityField"/>
</owl:NamedIndividual>


</rdf:RDF>
